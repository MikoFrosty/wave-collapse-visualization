{
  "name": "wave collapse visualizer",
  "version": "1.0.0",
  "description": "Visualizing wave collapse functions for building game maps.",
  "license": "MIT",
  "author": "Brandon Mikowski",
  "keywords": [
    "react",
    "boilerplate",
    "typescript",
    "starter",
    "vite",
    "redux",
    "material-ui",
    "rrd",
    "prettier"
  ],
  "scripts": {
    "dev": "vite --open",
    "start": "vite --open",
    "host": "vite --open --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,css,scss}\" --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cover": "vitest run --coverage",
    "test:e2e": "pnpm preview:test 'cypress open'",
    "test:e2e:headless": "pnpm preview:test 'cypress run'",
    "test:e2e:ci": "vite build && pnpm preview:test 'cypress run --record'"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.1",
    "@reduxjs/toolkit": "^1.9.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
    "vitest": "^0.33.0"
  }
}
